#include <stdio.h>
#include <math.h>

int main() {
    
    int HorasT;
    int PrecioH;
    int TotalPagoCorrespondiente;
    
    printf("Ingresa tus horas trabajadas: ");
    scanf("%d", &HorasT);
    printf("Ingresa el Costo por hora: ");
    scanf("%d", &PrecioH);
    
    TotalPagoCorrespondiente = HorasT*PrecioH;
    
       printf("Total de pago: %d\n", TotalPagoCorrespondiente);
    
    return 0;
}
