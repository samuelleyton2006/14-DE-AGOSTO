#include <stdio.h>

int main() {
    int edad;
    int precio = 0;

    printf("Ingrese la edad del cliente: ");
    scanf("%d", &edad);

    if (edad < 4) {
        precio = 0;
    } else if (edad >= 4 && edad <= 18) {
        precio = 5;
    } else {
        precio = 10;
    }

    printf("El precio de la entrada es: $%d\n", precio);

    return 0;
}
