#include <stdio.h>
#include <stdlib.h>

int main(){
    char contra[100], contraG[100];
    int igualdad=0;
    printf("Cree una contrase単a: ");
    scanf("%s", contra);
    
    printf("Ingrese la contrase単a creada: ");
    scanf("%s", contraG);
    
    
    for (int i=0;i<10;i++){
      if(contra[i]!=contraG[i])
        igualdad=-1;
}
    if(igualdad==0){
        printf("Contrase単a existente");
    }
    else{
        printf("Contrase単a no existente");
    }
    
    return 0;
}
